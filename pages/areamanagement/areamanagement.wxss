/* pages/areamanagement/areamanagement.wxss */
.arearoot {
  background: #f5f5f5;
  height: 100%;
  overflow-y: auto;
}

.content {
  margin-bottom: 70px;
}

.cell {
  padding-top: 10px;
  box-sizing: border-box;
}

.tag {
  width: 32px;
  height: 16px;
  display: flex;
  justify-content: center;
  align-items: center;
}


.add_button {
  height: 60px;
  padding: 0px 16px;
  box-sizing: border-box;
  line-height: 60px;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
}